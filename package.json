{
    "name": "selfNoticeService",
    "version": "0.0.6",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "dev:once": "nest start",
        "dev": "nest start --watch",
        "dev:debug": "nest start --debug --watch",
        "dev:prod": "nest build && node dist/main.js",
        "build": "nest build",
        "release": "nest build && pm2 start --name selfNoticeService dist/main.js",
        "prebuild": "rimraf dist",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs,test,utils,common}/**/*.{js,jsx,ts,tsx,less,css}\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "prepare": "husky install",
        "lint-staged": "lint-staged"
    },
    "dependencies": {
        "@nestjs/bull": "^0.4.1",
        "@nestjs/common": "^7.0.0",
        "@nestjs/config": "^0.5.0",
        "@nestjs/core": "^7.0.0",
        "@nestjs/mongoose": "^7.2.0",
        "@nestjs/platform-express": "^7.0.0",
        "@nestjs/swagger": "^4.5.7",
        "@nestjs/typeorm": "^7.1.4",
        "bull": "^3.28.1",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "crypto-js": "^4.0.0",
        "express-rate-limit": "^5.1.3",
        "helmet": "^3.22.0",
        "lodash": "^4.17.15",
        "mathjs": "^10.0.0",
        "moment": "^2.28.0",
        "nestjs-config": "^1.4.7",
        "nestjs-redis": "^1.2.8",
        "qiniu": "^7.3.2",
        "redis": "^3.0.2",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.5.4",
        "sha1": "^1.1.1",
        "swagger-ui-express": "^4.1.4",
        "typeorm": "^0.2.28"
    },
    "devDependencies": {
        "@commitlint/cli": "^15.0.0",
        "@commitlint/config-conventional": "^15.0.0",
        "@nestjs/cli": "^7.0.0",
        "@nestjs/schematics": "^7.0.0",
        "@nestjs/testing": "^7.0.0",
        "@types/bull": "^3.15.3",
        "@types/express": "^4.17.3",
        "@types/jest": "26.0.10",
        "@types/node": "^13.9.1",
        "@types/supertest": "^2.0.8",
        "@typescript-eslint/eslint-plugin": "3.9.1",
        "@typescript-eslint/parser": "3.9.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "cross-env": "^5.2.0",
        "eslint": "^7.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": ">=7",
        "jest": "^27.2.5",
        "lint-staged": "^12.1.2",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "^27.0.3",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "^3.10.1",
        "typeorm-model-generator": "^0.4.3",
        "typescript": "^4.3.5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "{src,apps,libs,test,utils,common}/**/*.{js,jsx,ts,tsx,less,css}": [
            "eslint --fix",
            "git add"
        ]
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
